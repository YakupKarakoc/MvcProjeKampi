@using EntityLayer.Concreate
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>HAKKIMDA VERİ LİSTESİ</h2>
<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif yap</th>
        <th>Pasif yap</th>
    </tr>
    @foreach(var item in Model)
			{
                <tr>
                    <td>@item.AboutDetails1</td>
                    <td>@item.AboutDetails2</td>
                    <td><a href="#" class="btn btn-warning">Aktif yap</a></td>
                    <td><a href="#" class="btn btn-warning">Pasif yap</a></td>

                </tr>
			}
</table>
@Html.Action("AboutPartial","About")    